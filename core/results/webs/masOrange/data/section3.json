{
    "title": "Commited Utilization",
    "description": "FinOps Google +Orange",
    "templates": [
        {
            "type": "cost-chart",
            "tipo": "BarChart",
            "dataSource": "data/gcp_commitment_utilization_query_results.json",
            "costReal": true,
            "realCostField": "total_cost",
            "groupBy": "service_description",
            "aggregation": "sum",
            "limit": 10,
            "orderBy": "total_cost",
            "orderDirection": "desc"
        },
        {
            "type": "DoughnutChart",
            "tipo": "Doughnut",
            "dataSource": "data/gcp_commitment_utilization_query_results.json",
            "costReal": true,
            "realCostField": "total_cost",
            "groupBy": "service_description",
            "aggregation": "sum",
            "limit": 10,
            "orderBy": "total_cost",
            "orderDirection": "desc"
        },
        {
            "type": "table",
            "dataSource": "data/gcp_commitment_utilization_query_results.json",
            "columns": [
                "service_description",
                "total_cost",
                "on_demand_cost",
                "month",
                "currency"
            ]
        }
    ]
}