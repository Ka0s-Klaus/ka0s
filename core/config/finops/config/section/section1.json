{
    "title": "Inicio",
    "description": "Bienvenido al dashboard de Ka0s",
    "templates": [
        {
            "type": "cost-chart",
            "tipo": "linechart",
            "title": "An√°lisis de Costos Diarios GCP",
            "dataSource": "gcp_anomalous_spending_query_results.json",
            "head": true,
            "metrics": [
                {
                    "field": "z_score",
                    "label": "Z-Score",
                    "type": "line"
                },
                {
                    "field": "daily_cost",
                    "label": "Costo Diario",
                    "type": "bar"
                },
                {
                    "field": "avg_daily_cost",
                    "label": "Costo Promedio",
                    "type": "line"
                }
            ],
            "groupBy": "day",
            "aggregation": "sum",
            "limit": 31,
            "orderBy": "day",
            "orderDirection": "desc"
        },
        {
            "type": "cost-chart",
            "tipo": "barchart",
            "title": "Proyectos Principales",
            "dataSource": "results/bq/data/gcp_anomaly_detection_query_results.json",
            "costReal": true,
            "realCostField": "total_cost",
            "costEstimated": true,
            "estimatedCostField": "avg_cost",
            "groupBy": "service_description",
            "aggregation": "sum",
            "limit": 10,
            "orderBy": "total_cost",
            "orderDirection": "asc"
        },
        {
            "type": "cost-chart",
            "tipo": "barchart",
            "title": "Servicios Principales",
            "dataSource": "results/bq/data/gcp_anomaly_prediction_query_results.json",
            "costReal": true,
            "realCostField": "projected_cost",
            "costEstimated": true,
            "estimatedCostField": "stddev_cost",
            "groupBy": "service_description",
            "aggregation": "sum",
            "limit": 10,
            "orderBy": "projected_cost",
            "orderDirection": "desc"
        }
    ]
}