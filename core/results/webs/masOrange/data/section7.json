{
    "title": "Crefit Breakdown",
    "description": "Bienvenido al dashboard de Ka0s",
    "templates": [
        {
            "type": "cost-chart",
            "tipo": "BarChart",
            "dataSource": "data/gcp_credit_breakdown_query_results.json",
            "costReal": true,
            "realCostField": "credit_amount",
            "groupBy": "credit_name",
            "aggregation": "sum",
            "limit": 10,
            "orderBy": "credit_amount",
            "orderDirection": "asc"
        },
        {
            "type": "DoughnutChart",
            "tipo": "BarChart",
            "dataSource": "data/gcp_credit_breakdown_query_results.json",
            "costReal": true,
            "realCostField": "credit_amount",
            "groupBy": "credit_name",
            "aggregation": "sum",
            "limit": 10,
            "orderBy": "credit_amount",
            "orderDirection": "asc"
        },
        {
            "type": "table",
            "dataSource": "data/gcp_credit_breakdown_query_results.json",
            "columns": [
                "credit_name",
                "credit_amount",
                "credit_type",
                "month",
                "currency"
            ]
        }
    ]
}