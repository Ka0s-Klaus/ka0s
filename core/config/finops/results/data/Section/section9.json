{
    "title": "Monthly Cost",
    "description": "Bienvenido al dashboard de Ka0s",
    "templates": [
        {
            "type": "cost-chart",
            "tipo": "BarChart",
            "dataSource": "data/gcp_monthly_cost_summary.json",
            "costReal": true,
            "realCostField": "final_cost",
            "costEstimated": true,
            "estimatedCostField": "total_cost",
            "groupBy": "service_description",
            "aggregation": "sum",
            "limit": 10,
            "orderBy": "final_cost",
            "orderDirection": "desc"
        },
        {
            "type": "DoughnutChart",
            "tipo": "BarChart",
            "dataSource": "data/gcp_monthly_cost_summary.json",
            "costReal": true,
            "realCostField": "final_cost",
            "costEstimated": true,
            "estimatedCostField": "total_cost",
            "groupBy": "service_description",
            "aggregation": "sum",
            "limit": 10,
            "orderBy": "final_cost",
            "orderDirection": "desc"
        },
        {
            "type": "table",
            "dataSource": "data/gcp_monthly_cost_summary.json",
            "columns": [
                "service_description",
                "total_cost",
                "final_cost",
                "month",
                "currency"
            ]
        }
    ]
}