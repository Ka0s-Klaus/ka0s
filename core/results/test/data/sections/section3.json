{
    "title": "Lead Time",
    "description": "Estado y lista de workflows disponibles",
    "templates": [
        {
            "type": "summary",
            "dataSource": "data/kaos-workflows-runs.json",
            "metrics": [
                {
                    "name": "totalItems",
                    "type": "count",
                    "title": "Total"
                },
                {
                    "name": "activeCount",
                    "type": "count",
                    "title": "In Progress",
                    "filter": {
                        "field": "status",
                        "value": "in_progress"
                    }
                },
                {
                    "name": "disabledCount",
                    "type": "count",
                    "title": "Compleatados",
                    "filter": {
                        "field": "status",
                        "value": "completed"
                    }
                }
                ,
                {
                    "name": "disabledCount",
                    "type": "count",
                    "title": "Rama Principal",
                    "filter": {
                        "field": "head_branch",
                        "value": "main"
                    }
                }
            ]
        },
        {
            "type": "table",
            "title": "Ka0s Modules",
            "dataSource": "data/kaos-workflows-available.json",
            "columns": [
                "id",
                "name",
                "state"
            ]
        }
    ]
}