{
    "title": "Inicio",
    "description": "Bienvenido al dashboard de Ka0s",
    "templates": [
        {
            "type": "cost-chart",
            "tipo": "StackedBarChart",
            "title": "Costos GCP",
            "dataSource": "../data/gcp_cost_2025.json",
            "head": true,
            "costReal": true,
            "costEstimated": true,
            "groupBy": "month",
            "aggregation": "sum",
            "currentYear": true
        },
        {
            "type": "cost-chart",
            "tipo": "BarChart",
            "title": "Proyectos Principales",
            "dataSource": "../data/gcp_cost_all_year.json",
            "costReal": true,
            "costEstimated": true,
            "groupBy": "id",
            "aggregation": "sum",
            "limit": 10,
            "orderBy": "cost",
            "orderDirection": "desc"
        },
        {
            "type": "cost-chart",
            "title": "Servicios Principales",
            "dataSource": "../data/gcp_cost_all_year.json",
            "costReal": true,
            "costEstimated": true,
            "groupBy": "name",
            "aggregation": "sum",
            "limit": 7,
            "orderBy": "cost",
            "orderDirection": "desc"
        }
    ]
}