{
    "title": "Monthly Cost",
    "description": "Bienvenido al dashboard de Ka0s",
    "templates": [
        {
            "type": "cost-chart",
            "tipo": "BarChart",
            "dataSource": "../data/gcp_monthly_cost_summary_query_results.json",
            "costReal": true,
            "realCostField": "final_cost",
            "costEstimated": true,
            "estimatedCostField": "total_cost",
            "groupBy": "service_description",
            "aggregation": "sum",
            "limit": 10,
            "orderBy": "final_cost",
            "orderDirection": "desc"
        },
        {
            "type": "DoughnutChart",
            "tipo": "BarChart",
            "dataSource": "../data/gcp_monthly_cost_summary_query_results.json",
            "costReal": true,
            "realCostField": "final_cost",
            "costEstimated": true,
            "estimatedCostField": "total_cost",
            "groupBy": "service_description",
            "aggregation": "sum",
            "limit": 10,
            "orderBy": "final_cost",
            "orderDirection": "desc"
        },
        {
            "type": "table",
            "dataSource": "../data/gcp_monthly_cost_summary_query_results.json",
            "columns": [
                "description",
                "final_cost",
                "net_cost",
                "total_credits",
                "usage_date"
            ]
        }
    ]
}