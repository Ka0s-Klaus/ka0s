{
    "title": "Inicio",
    "description": "Bienvenido al dashboard de Ka0s",
    "templates": [
        {
            "type": "cost-chart",
            "tipo": "bar",
            "title": "Costos por Servicio GCP",
            "dataSource": "gcp_anomalous_spending_query_results.json",
            "head": true,
            "costReal": true,
            "realCostField": "daily_cost",
            "costEstimated": true,
            "estimatedCostField": "avg_daily_cost",
            "groupBy": "day", 
            "aggregation": "sum",
            "limit": 31,      
            "orderBy": "daily_cost",  
            "orderDirection": "asc"
        },
        {
            "type": "cost-chart",
            "tipo": "bar",
            "title": "Proyectos Principales",
            "dataSource": "results/bq/data/gcp_anomaly_detection_query_results.json",
            "costReal": true,
            "realCostField": "total_cost",
            "costEstimated": true,
            "estimatedCostField": "avg_cost",
            "groupBy": "service_description",
            "aggregation": "sum",
            "limit": 10,
            "orderBy": "total_cost",
            "orderDirection": "asc"
        },
        {
            "type": "cost-chart",
            "tipo": "BarChart",
            "title": "Servicios Principales",
            "dataSource": "results/bq/data/gcp_anomaly_prediction_query_results.json",
            "costReal": true,
            "realCostField": "projected_cost",
            "costEstimated": true,
            "estimatedCostField": "stddev_cost",
            "groupBy": "service_description",
            "aggregation": "sum",
            "limit": 10,
            "orderBy": "projected_cost",
            "orderDirection": "desc"
        }
    ]
}